    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config jest-e2e.json",
    "seed:users": "ts-node -r tsconfig-paths/register src/database/seeds/seed-users.ts",
    "seed:vitals": "ts-node -r tsconfig-paths/register src/database/seeds/seed-vitals.ts",
    "seed:care": "ts-node -r tsconfig-paths/register src/database/seeds/seed-histora-care-complete.ts",
    "seed:all": "npm run seed:users && npm run seed:vitals && npm run seed:care",
    "db:cleanup": "ts-node -r tsconfig-paths/register src/database/seeds/cleanup-test-users.ts"
  },
